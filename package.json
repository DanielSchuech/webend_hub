{
  "name": "webend_hub",
  "version": "0.0.1",
  "description": "Frontend Hub For Webend",
  "main": "build/server/server.js",
  "scripts": {
    "buildApp": "cd src && copyfiles app/index.html ../build  && copyfiles app/main.ts ../build",
    "copyJSONs": "cd src && copyfiles *.json **/*.json ../build",
    "compile": "tsc",
    "build": "npm run compile && npm run copyJSONs && npm run buildApp",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "webendConfig": {
    "port": 3001,
    "pathToNodeModules": "../"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielSchuech/webend_hub.git"
  },
  "keywords": [
    "webend",
    "hub",
    "frontend"
  ],
  "author": "Daniel Schuech",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanielSchuech/webend_hub/issues"
  },
  "homepage": "https://github.com/DanielSchuech/webend_hub#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular": "^1.5.5",
    "es6-shim": "^0.35.0",
    "express": "^4.13.4",
    "ngAdapter": "0.0.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "tiny-di": "^0.3.2",
    "ts-loader": "^0.8.2",
    "webpack": "^1.13.0",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "copyfiles": "^0.2.1",
    "tslint": "^3.8.1",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
